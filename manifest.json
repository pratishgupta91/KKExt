{
  "manifest_version": 2,

  "name": "Kaur Homepage",
  "description": "This extension will make you happy",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
 "chrome_url_overrides" : {
    "newtab": "popup.html"
  },
  "permissions": [
   "activeTab",
   "storage"
   ]
}