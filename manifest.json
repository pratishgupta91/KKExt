{
  "manifest_version": 2,

  "name": "WallNote",
  "description": "Keeps you less forgetful.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "w.png" 
  },

 "chrome_url_overrides" : {
    "newtab": "experiment.html"
  },

  "permissions": [
   "activeTab",
   "storage",
   "notifications"
   ]
}